<apex:page showheader="False" standardstylesheets="false" title="Store Admin" controller="wk_wedge.StoreAdminController" doctype="html-5.0" action="{!checkMetadataAPIConnection}">
    <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"/>
    <apex:includeScript value="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"/>
    <style>
        .wdg-nav-icon.storeadmin {
        background-color: #34becd;
        }
        .wdg-nav-icon.cms {
        background-color: #34becd;
        }
        .wdg-Notedata{
        font-size:12px;
        color:red;
        }
        
        .nav-tabs { 
        border-bottom: 2px solid #DDD;
        padding: 0px;
        }
        .nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover {
        border-width: 0;
        }
        .nav-tabs > li {
        display:inline-block;
        }
        .nav-tabs > li > a {
        border: none;
        display:block;
        padding: 10px 15px;
        color: #666;
        }
        .nav-tabs > li.active > a, .nav-tabs > li > a:hover {
        border: none;
        color: white !important;
        background: #FF8C44 ;
        }
        .nav-tabs > li > a::after {
        content: "";
        background: #4285F4;
        height: 2px; position: absolute;
        width: 100%;
        left: 0px;
        bottom: -1px;
        transition: all 250ms ease 0s;
        transform: scale(0); 
        }
        .nav-tabs > li.active > a::after, .nav-tabs > li:hover > a::after {
        transform: scale(1);
        }
        .tab-nav > li > a::after {
        background: #21527d none repeat scroll 0% 0%;
        color: #fff;
        }
        .tab-pane {
        padding: 15px 0;
        display:none;
        }
        .tab-content{
        padding:20px
        }
        
        .tab-content > .active{
        display:block !important;
        }
        
        .card {
        background: #FFF none repeat scroll 0% 0%;
        box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.3);
        margin-bottom: 30px;
        }
        .hidden{
        display:none !important;
        }
        .web-analytic-label {
        width: 22%;
        padding: 15px;
        color: #555555;
        font-size: 14px;
        text-align: right;
        vertical-align: top;
        }
        .web-analytic-input {
        padding: 6px;
        border-radius: 3px;
        font-size: 14px;
        color: #222;
        border: 1px solid #adadad;
        outline: none;
        box-shadow: none;
        display: block;
        width: 100%;
        box-sizing: border-box;
        }
        .wdg-form--table tbody td.field select.display-inline--block, .wdg-form--table tbody td.field input[type="text"].display-inline--block{
        width: 100%;
        }
        /* #pushstatus,#pushNotiKeyPanel,#pushImage,#reqstatus,#reqKeyPanel,#rmaKeyPanel{
        display:none;
        }
        #userKeyPanel,#status,*/
        .wdg-form--table input[type="text"]{
        width:auto;
        display:inline-block;
        }    
        .wdg-addOption--btn{
        margin-left: 10px;
        }
        .wedge_selectListOuter{
        max-height: 200px;
        }
        /*.wdg-form--box .wdg-form--content{
        height:620px;
        }*/
    </style>
