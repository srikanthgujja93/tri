            <script>
            function noenter(ev, obj)  {
                if (window.event && window.event.keyCode == 13 || ev.which == 13) {
                    if(obj == 'clickPortal') {
                        console.log($('input[id$="queryPortalName"]').val());
                        clickPortal($('input[id$="queryPortalName"]').val()); 
                    } else if(obj == 'clickRegister') {
                        clickRegister($('input[id$="queryRegisterName"]').val());
                    } else if(obj == 'clickGuest') {
                        clickGuest($('input[id$="queryGuestName"]').val());
                    } else if(obj == 'clickUser') {
                        clickUser($('input[id$="queryUserName"]').val());
                    }else if(obj == 'clickPb') {
                        clickPb($('input[id$="queryPriceBookName"]').val());
                    }else if(obj == 'clickArticle') {
                        clickArticle($('input[id$="queryArticleName"]').val());
                    }else if(obj == 'clickRma') {
                        clickRma($('input[id$="queryRmaName"]').val());
                    }
                    
                    return false;
                } else {
                    return true;
                }
            }
            function delay(obj){
                var interval = 0;
                if (interval)
                    window.clearTimeout(interval);
                interval = window.setTimeout(function() {
                    if(obj == 'clickPortal') {
                        console.log($('input[id$="queryPortalName"]').val());
                        clickPortal($('input[id$="queryPortalName"]').val()); 
                    } else if(obj == 'clickRegister') {
                        clickRegister($('input[id$="queryRegisterName"]').val());
                    } else if(obj == 'clickGuest') {
                        clickGuest($('input[id$="queryGuestName"]').val());
                    } else if(obj == 'clickUser') {
                        clickUser($('input[id$="queryUserName"]').val());
