            function payUmoneyPayment(){
                var exRate = '{!exchangeRate}';
                if(exRate == '')
                    exRate = 1;
                else
                    exRate = parseFloat(exRate);
                console.log($('[Id$="totalPay"]').text());
                var totalAmt = $('[Id$="totalPay"]').text().replace(/,/g,'');
