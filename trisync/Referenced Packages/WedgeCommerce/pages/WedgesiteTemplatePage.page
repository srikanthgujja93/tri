            </apex:outputPanel>
            <apex:outputpanel layout="block" id="makeanofferview" styleclass="productQview offer">
                <apex:outputpanel layout="block" rendered="{!makeanofferview}">
                    <div class="wdg-status--loaderbg"> 
                        <div class="productQview-wrap requestQoute">
                            <p class="productQview-header">
                                <apex:outputText value="{!$Label.wk_wedge__site_template_request_quote_popup}"/>
                                <span class="productQview-close offer" id="details-close" > ×</span>
                            </p>
                            <div class="productQview-details requestQoute">
                                <apex:repeat value="{!requestQuoteCustomField}" var="key">
                                    <div style="margin-bottom:3px;"><b>{!key.name}<apex:outputPanel rendered="{!key.wk_wedge__Required__c}" style="color: #ff0000">*</apex:outputPanel>&nbsp;:</b></div>
                                    <div>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='S'}" layout="block">
                                            <select class="wdg-offer-selectlist selectType"  var-name="{!key.name}" req="{!key.wk_wedge__Required__c}">
                                                <apex:repeat value="{!key.Request_Quote_Custom_Field_Variants__r}" var="subkey">
                                                    <option value="{!subkey.name}">{!subkey.name}</option>
                                                </apex:repeat>
                                            </select>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='R'}" layout="block">
                                            <apex:repeat value="{!key.Request_Quote_Custom_Field_Variants__r}" var="subkey">
                                                <input type="radio" value="{!subkey.name}" name="{!key.Id}"  var-name="{!key.name}" req="{!key.wk_wedge__Required__c}" class="wdg-offer-radio" /> <span class="wdg-offer-radioOptions">{!subkey.name}</span>       
                                            </apex:repeat>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='N'}" layout="block" >
                                            <apex:repeat value="{!key.Request_Quote_Custom_Field_Variants__r}" var="subkey">
                                                <div class="custom-checkbox">
                                                    <input type="checkbox" value="{!subkey.name}" class="wdg-offer-multiCheck" req="{!key.wk_wedge__Required__c}" var-name="{!key.name}"/> <span class="wdg-offer-options">{!subkey.name}</span>
                                                </div>
                                            </apex:repeat>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='M'}" layout="block">
                                            <select multiple="true" class="wdg-offer-multiSelect" var-name="{!key.name}" req="{!key.wk_wedge__Required__c}">
                                                <apex:repeat value="{!key.Request_Quote_Custom_Field_Variants__r}" var="subkey">
                                                    <option value="{!subkey.name}">{!subkey.name}</option>
                                                </apex:repeat>
                                            </select>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='C'}" layout="block" styleClass="custom-checkbox">
                                            <input type="checkbox" class="wdg-offer-check" var-name="{!key.name}" req="{!key.wk_wedge__Required__c}"/>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='I'}" layout="block">
                                            <input type="text" class="wdg-offer-inputField" var-name="{!key.name}" req="{!key.wk_wedge__Required__c}"/>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='T'}" layout="block">
                                            <textarea class="wdg-offer-textarea" var-name="{!key.name}" req="{!key.wk_wedge__Required__c}"></textarea>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='V'}" layout="block">
                                            <input type="Date" class="wdg-offer-Date" var-name="{!key.name}" req="{!key.wk_wedge__Required__c}"/>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='Y'}" layout="block">
