      window.rootcategories=function (data){
          var begin='';
          for(var item=0;item<data.length;item++){
              if(data[item].alias!==undefined && data[item].title!==undefined){
                  begin+='<li><a href="'+siteurl+'/'+data[item].alias+'">'+data[item].title+'</a>';
                  
                  if(data[item].children!==undefined){
                      var count=0;
                      var childData=data[item].children;
                      for(child in childData){
                          if(childData[child].children!==undefined)
                              count+=1;
                      }
                      if(count>0){
                          begin+='<div class="nav-wrap"><ul class="wdg-root--nav" style="left:auto;">'
                          begin+=childCategories(data[item].children);
                          begin+='</div></ul>';
                      }    
                      else{
                          begin+='<div class="nav-wrap"><ul class="wdg-root--nav" style="left:auto;">';
                          begin+=subChildCategories(data[item].children);
                          begin+='</div></ul>';
                      } 
                      
                  }
                  
                  begin+='</li>';
              }
          }
          begin+=''
          return begin;
