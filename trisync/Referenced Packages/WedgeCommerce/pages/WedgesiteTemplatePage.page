                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!key.wk_wedge__Element_Type__c=='P'}" layout="block">
                                            <input type="text" class="wdg-offer-number" var-name="{!key.name}" req="{!key.wk_wedge__Required__c}"/>
                                        </apex:outputPanel>                                        
                                    </div>
                                </apex:repeat>
